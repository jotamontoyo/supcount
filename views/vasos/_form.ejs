    <div class="form-group">
        <label for="nombre" class="col-md-3 control-label">Nombre:</label>
        <div class="col-md-9">
            <input type="text" class="form-control" id="nombre" name="vaso[nombre]" value="<%= vaso.nombre %>" placeholder="nombre"/>
        </div>
    </div>

    <div class="form-group">
        <label for="ubicacion" class="col-md-3 control-label">Ubicación:</label>
        <div class="col-md-9">
            <input type="text" class="form-control" id="ubicacion" name="vaso[ubicacion]" value="<%= vaso.ubicacion %>" placeholder="ubicacion"/>
        </div>
    </div>

    <div class="form-group">
        <label for="capacidad" class="col-md-3 control-label">Capacidad:</label>
        <div class="col-md-9">
            <input type="text" class="form-control" id="capacidad" name="vaso[capacidad]" value="<%= vaso.capacidad %>" placeholder="capacidad"/>
        </div>
    </div>

    <div class="form-group">
        <label for="interior" class="col-md-3 control-label">Interior:</label>
        <div class="col-md-1">
            <% if (vaso.interior) { %>
                <input type="checkbox" class="form-control" checked id="interior" name="vaso[interior]" value="<%= vaso.interior %>" />
            <%} else { %>
                <input type="checkbox" class="form-control" id="interior" name="vaso[interior]" value="<%= vaso.interior %>" />
            <%}%>
        </div>
    </div>



    <div class="form-group">
