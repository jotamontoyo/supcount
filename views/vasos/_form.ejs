        <div class="col-sm-3">
            <select class="form-control" id="tipo_ensayo" name="vaso[tipo_ensayo]">
                <option value="Completo" <%if (vaso.tipo_ensayo==="Completo") {%> selected <%}%>>Completo</option>
                <option value="Parcial" <%if (vaso.tipo_ensayo==="Parcial") {%> selected <%}%>>Parcial</option>
            </select>
        </div>
    </div>



    <br>

    <div class="col-md-6">
